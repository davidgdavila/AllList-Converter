Principal:
#Version2:
<Principal>
    eti1: eti1
    barra: barra
    cbox_dyno: cbox_dyno
    cbox_versio: cbox_versio
    MDToolbar:
        title: "All List-Converter"
        pos_hint: {"top": 1}
        right_action_items : [["folder", lambda x : root.seleccionar_archivo()],["account-question", lambda x : app.show_acerca_de()]]
    MDBoxLayout:

        MDLabel:
            text: "    Listas a realizar :"
            size_hint: None, None
            size: "150dp", "48dp"
            halign: "left"
            pos_hint: {"top": .90}
        MDCheckbox:
            id: cbox_dyno
            size_hint: None, None
            size: "30dp", "48dp"
            halign: "left"
            pos_hint: {"top": .90}
            active: root.estado_cbox_dyno
        MDLabel:
            text: "Dyno"
            size_hint: None, None
            size: "48dp", "48dp"
            halign: "left"
            color:"black"
            pos_hint: {"top": .90}
        MDCheckbox:
            id: cbox_versio
            size_hint: None, None
            size: "30dp", "48dp"
            halign: "left"
            pos_hint: {"top": .90}
            active: root.estado_cbox_versio
        MDLabel:
            text: "Versio"
            size_hint: None, None
            size: "48dp", "48dp"
            halign: "left"
            color:"black"
            pos_hint: {"top": .90}

    ProgressBar:
        id: barra
        size_hint: (1,.1)
        max:99
        min:0
        pos_hint: {"top": .88}
    MDLabel:
        id: eti1
        halign: "center"
        text: "Arrastra y suelta la pauta de continuidad aqui..."
        color: 0,0,0, 0.5
        italic: True
        #pos_hint: "500dp"

<Acerca_de>
    size_hint: (None,None)
    size: "400dp", "300dp"
    MDLabel:
        adaptive_size: True
        halign: "center"
        pos_hint: {"top": 1}
        text: "Autor: David Israel González Dávila"
    MDFlatButton:
        text: "okok"
        on_release: self.cerrar()



<Version2>
